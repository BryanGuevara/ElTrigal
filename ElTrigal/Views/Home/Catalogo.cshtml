@{
    ViewData["Title"] = "Catalogo";
    Layout = null;
}
<link rel="stylesheet" href="~/css/index.css">
@if (RoleContext.CurrentRole != null && !RoleContext.CurrentRole.VerCatalogo)
{
    <script>window.location.href = '@Url.Action("Index", "Home")';</script>
}
<div class="container">

    @foreach (var item in ViewBag.Proveedores)
        {
        <a class="nav-link text-dark catalogo dentro" asp-controller="Home" asp-action="VerCatalogo" asp-route-id="@item.Id">@item.Nombre</a>
        }     

    <a class="nav-link text-dark salir dentro" asp-controller="Home" asp-action="Index">Volver</a>
</div>
